{
  "homepage": "https://suyogagitayana.github.io/steam-sale/",
  "name": "steam-sale",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.34.0",
    "gh-pages": "5.0.0",
    "tailwindcss": "^3.2.7"
  },
  "scripts": {
    "start:js": "react-scripts start",
    "start:wind": "npx tailwindcss -i ./src/assets/css/tailwind/wind.css -o ./src/assets/css/tailwind/index.css --watch",
    "start:fix": "npx eslint --fix ./src || echo \"Formatting done! Please fix the remaining warnings and errors\"",
    "start": "npm run start:js && npm run start:wind",
    "build:wind": "npx tailwindcss -o ./src/assets/css/tailwind/index.css --minify",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
